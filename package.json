{
  "name": "ui-lib",
  "description": "UI Library | Vue3, TS, Jest, SCSS, ESBuild",
  "version": "0.0.1",
  "main": "./dist/entry.umd.js",
  "module": "./dist/entry.es.js",
  "types": "./dist/types/src/entry.d.ts",
  "exports": {
    ".": {
      "import": "./dist/entry.es.js",
      "require": "./dist/entry.umd.js"
    }
  },
  "files": [
    "./dist",
    "./src"
  ],
  "sideEffects": [
    "*.css",
    "*.scss",
    "*.sass"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "prepack": "npm run build"
  },
  "dependencies": {
    "vue": "3.x"
  },
  "devDependencies": {
    "@types/node": "18.11.7",
    "@vitejs/plugin-vue": "3.2.0",
    "typescript": "4.8.4",
    "vite": "3.2.1",
    "vue-tsc": "1.0.9"
  },
  "peerDependencies": {
    "vue": "3.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matim-ioioi/ui-lib.git"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matim-ioioi/ui-lib/issues"
  },
  "homepage": "https://github.com/matim-ioioi/ui-lib#readme"
}
